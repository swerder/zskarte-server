{
  "kind": "collectionType",
  "collectionName": "organizations",
  "info": {
    "singularName": "organization",
    "pluralName": "organizations",
    "displayName": "Organization",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "mapLongitude": {
      "type": "decimal"
    },
    "mapLatitude": {
      "type": "decimal"
    },
    "mapZoomLevel": {
      "type": "decimal"
    },
    "defaultLocale": {
      "type": "enumeration",
      "enum": [
        "de-CH",
        "fr-CH",
        "it-CH",
        "en-US"
      ],
      "default": "de-CH"
    },
    "url": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "operations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::operation.operation",
      "mappedBy": "organization"
    },
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "organization"
    }
  }
